{
    "version": "1.0",
    "cwd": "/",
    "args": [
        "cat",
        "/files/version.txt"
    ],
    "env": [
    ],
    "user": {
        "uid": 1000,
        "gid": 1000
    },
    "console": {
        "limit": 65536,
        "path": "/tmp/mounts-xi6.log"
    },
    "etc": {
        "group": [
            "root:x:0:"
        ],
        "passwd": [
            "root::0:0:root:/:/bin/false"
        ]
    },
    "memLimit": 41943040,
    "network": "nat",
    "mounts": [{
        "destination": "/files",
        "options": ["rbind", "ro", "nosuid", "nodev"],
        "source": "/",
        "type": "bind"
    }]
}